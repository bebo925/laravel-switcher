{
    "name": "livewire-switcher",
    "displayName": "Livewire Switcher",
    "description": "Switch between component class and view files for Laravel Livewire",
    "version": "0.0.1",
    "publisher": "bebo925",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bebo925/laravel-switcher.git"
    },
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Keymaps",
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:livewire-switcher.switch"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "livewire-switcher.switch",
                "title": "Livewire: Switch Component File"
            }
        ],
        "keybindings": [
            {
                "command": "livewire-switcher.switch",
                "key": "alt+ctrl+l",
                "mac": "cmd+alt+l"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.64.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3"
    }
}
